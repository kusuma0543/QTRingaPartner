<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.ringaapp.ringapartner.activities.UploadPartnerServImages">

    <android.support.design.widget.AppBarLayout
        android:id="@+id/toolbarzx"

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme.AppBarOverlay">

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbarz"
            android:layout_width="match_parent"
            app:titleTextAppearance="@style/TextAppearance.TabsFont"
            android:layout_height="?attr/actionBarSize"
            android:background="?attr/colorPrimary"
            app:title="Service Images Upload"
            app:popupTheme="@style/AppTheme.PopupOverlay" />

    </android.support.design.widget.AppBarLayout>

    <include layout="@layout/content_upload_partner_serv_images" />


    <Button
        android:id="@+id/alluploadbut"
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:layout_height="45dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="2dp"
        android:background="@drawable/filter_shape"
        android:textAllCaps="false"
        android:fontFamily="@font/nunito_semibold"
        android:gravity="center"
        android:text="Submit"
        android:textColor="@color/colorWhite"
        android:textSize="18sp"
        android:textStyle="bold" />
</RelativeLayout>
