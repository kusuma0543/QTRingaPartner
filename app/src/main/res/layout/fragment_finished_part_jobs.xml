<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.ringaapp.ringapartner.FinishedPartJobs">
    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/partnerongoingjobs_listview"
        android:divider="@null"
        android:visibility="invisible"
        android:dividerHeight="0dp"
        />

    <pl.droidsonroids.gif.GifImageView
        android:id="@+id/gif_list"
        android:layout_width="200dp"
        android:layout_height="200dp"

        android:layout_gravity="top|center"
        android:layout_margin="10dp"
        android:src="@drawable/clipboard_new"

        />
    <TextView
        android:id="@+id/text_list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:text="Getting You Better Jobs! Stay Notified!"
        android:textColor="@color/colorPrimary"
        android:layout_gravity="center"
        android:textSize="18sp" />
</FrameLayout>
