<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.ringaapp.ringapartner.fragments.FinishedPartJobs">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Finished Leads"
        android:layout_gravity="center_horizontal"
        android:textSize="24sp"
        android:fontFamily="@font/nunito_semibold"
        android:layout_marginTop="5dp"
        android:textColor="@color/colorAccent"/>
    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/partnerongoingjobs_listview"
        android:divider="@null"
        android:layout_marginTop="45dp"

        android:visibility="invisible"
        android:dividerHeight="0dp"
        />

    <!--<pl.droidsonroids.gif.GifImageView-->
        <!--android:id="@+id/gif_list"-->
        <!--android:layout_width="200dp"-->
        <!--android:layout_height="200dp"-->
        <!--android:visibility="invisible"-->
        <!--android:layout_gravity="top|center"-->
        <!--android:layout_marginLeft="10dp"-->
        <!--android:layout_marginRight="10dp"-->
        <!--android:layout_marginTop="70dp"-->
        <!--android:src="@drawable/clipboard_new"-->

        <!--/>-->
    <ImageView
        android:id="@+id/gif_list"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_gravity="top|center"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:visibility="invisible"
        android:src="@drawable/main_image"
        android:layout_marginTop="70dp"/>
    <TextView
        android:id="@+id/text_list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="45dp"

        android:text="Getting You Better Jobs! Stay Notified!"
        android:textColor="@color/colorPrimary"
        android:layout_gravity="center"
        android:textSize="18sp" />

</FrameLayout>
